{
  "build": {
    "beforeDevCommand": "pnpm run font-slice && trunk serve",
    "beforeBuildCommand": "pnpm run font-slice && trunk build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": false
  },
  "productName": "client-installer",
  "version": "0.0.0",
  "identifier": "site.yuanshen.installer",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "KYT Inst",
        "resizable": false,
        "center": true,
        "width": 795,
        "height": 407,
        "transparent": true,
        "decorations": false,
        "dragDropEnabled": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}
