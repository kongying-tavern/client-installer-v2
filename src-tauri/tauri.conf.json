{
  "build": {
    "beforeDevCommand": "trunk serve",
    "beforeBuildCommand": "trunk build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "client-installer",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": false,
        "hide": true,
        "show": true,
        "minimize": true,
        "unminimize": true,
        "close": true,
        "startDragging": true
      }
    },
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "KYT Inst",
        "resizable": false,
        "center": true,
        "width": 795,
        "height": 407,
        "transparent": true,
        "decorations": false,
        "fileDropEnabled": false
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": false,
      "identifier": "site.yuanshen.installer"
    }
  }
}
